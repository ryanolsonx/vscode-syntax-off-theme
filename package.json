{
  "name": "ryanolsonxsyntaxoff",
  "version": "1.0.0",
  "displayName": "Without Syntax Highlighting",
  "description": "A minimal theme for Visual Studio Code that removes all syntax highlighting, providing a plain text experience.",
  "author": "Ryan Olson",
  "publisher": "ryanolsonx",
  "repository": "https://github.com/ryanolsonx/vscode-syntax-off-theme",
  "license": "MIT",
  "icon": "icon.png",
  "keywords": [
    "minimalism",
    "syntax off",
    "no syntax highlighting",
    "without syntax highlighting",
    "extreme minimalism",
    "plain text",
    "dark",
    "light"
  ],
  "engines": {
    "vscode": "^1.20.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Syntax Off",
        "uiTheme": "vs-dark",
        "path": "./themes/syntax-off.json"
      },
      {
        "label": "Syntax Off (Light)",
        "uiTheme": "vs",
        "path": "./themes/syntax-off-light.json"
      }
    ]
  }
}
